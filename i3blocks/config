[mpd]
command=~/.config/i3blocks/scripts/mpd.i3block
label=
interval=once
signal=1

[cpu]
command=sensors | grep Tctl | awk '{print $2}'
label=CPU 
interval=2

[gpu]
command=nvidia-smi | grep % | awk '{print$3}'
label=GPU 
interval=2

[mem]
command=free -m | grep Mem | awk '{print $7}'
label=RAM 
interval=3

[volume]
command=~/.config/i3blocks/scripts/alsa.i3block
label=ï€¨ 
instance=Master
interval=once
signal=10

[time]
command=date +"%a, %b %d %R"
interval=60

